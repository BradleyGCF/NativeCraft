{
	"name": "templatefront",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"start": "vite --host",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "biome format --write ./src",
		"lint:check": "biome lint ./src",
		"lint:apply": "biome check --write ./src",
		"prepare": "husky",
		"lint": "yarn format && yarn lint:check && yarn lint:apply",
		"test": "cypress run --spec ./cypress/e2e/* --browser chrome",
		"serve": "npx serve -s dist  -l 3000"
	},
	"dependencies": {
		"@radix-ui/react-alert-dialog": "1.1.1",
		"@radix-ui/react-avatar": "1.1.0",
		"@radix-ui/react-dialog": "1.1.1",
		"@radix-ui/react-icons": "1.3.0",
		"@radix-ui/react-popover": "1.1.1",
		"@radix-ui/react-slot": "1.1.0",
		"@tanstack/react-query": "^5.59.15",
		"axios": "1.7.3",
		"class-variance-authority": "0.7.0",
		"clsx": "2.1.1",
		"dotenv": "16.4.5",
		"formik": "2.4.6",
		"next-themes": "0.3.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-error-boundary": "4.0.13",
		"react-icons": "5.3.0",
		"react-router-dom": "6.26.0",
		"serve": "14.2.4",
		"sonner": "1.5.0",
		"tailwind-merge": "2.5.2",
		"tailwindcss-animate": "1.0.7",
		"vite-plugin-env": "1.0.1",
		"yup": "1.4.0",
		"zustand": "4.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@esbuild-plugins/node-globals-polyfill": "0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "0.2.2",
		"@types/cypress": "1.1.3",
		"@types/node": "22.2.0",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@vitejs/plugin-react": "4.3.1",
		"autoprefixer": "10.4.20",
		"cypress": "13.13.2",
		"husky": "9.1.4",
		"lint-staged": "15.2.8",
		"postcss": "8.4.41",
		"tailwindcss": "3.4.9",
		"ts-node": "10.9.2",
		"typescript": "5.5.4",
		"vite": "5.4.0",
		"vite-plugin-node-polyfills": "0.22.0",
		"vite-tsconfig-paths": "5.0.1",
		"vitest": "2.0.5"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome format --write --no-errors-on-unmatched",
			"biome lint --apply --no-errors-on-unmatched",
			"biome check --files-ignore-unknown=true",
			"biome check --apply --no-errors-on-unmatched",
			"biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
			"biome check --apply-unsafe --no-errors-on-unmatched"
		]
	},
	"engines": {
		"node": "18.20.5"
	},
	"packageManager": "yarn@1.22.22"
}
