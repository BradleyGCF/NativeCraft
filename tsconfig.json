{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "strict": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "target": "es6",
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "example"
  ],
  "extends": "expo/tsconfig.base"
}
